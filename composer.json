{
    "name": "danaid/dana-php",
    "description": "API for doing operations in DANA Payment Gateway (Gapura), Widget, Merchant Management, and Disbursement",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/dana-id/dana-php",
    "license": "unlicense",
    "authors": [
        {
            "name": "DANA Package Manager",
            "homepage": "https://github.com/dana-id/dana-php"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0",
        "vlucas/phpdotenv": "^5.5",
        "ramsey/uuid": "^4.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5",
        "php-webdriver/webdriver": "^1.15"
    },
    "autoload": {
        "psr-4": { 
            "Dana\\": "lib/"
        },
        "classmap": [
            "lib/PaymentGateway/v1/Enum/Enum.php",
            "lib/Widget/v1/Enum/Enum.php",
            "lib/MerchantManagement/v1/Enum/Enum.php",
            "lib/Disbursement/v1/Enum/Enum.php"
        ]
    },
    "autoload-dev": {
        "psr-4": { "Dana\\Tests\\" : "tests/" }
    }
}
